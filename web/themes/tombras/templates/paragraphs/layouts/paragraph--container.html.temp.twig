{%
  set classes = [
  'paragraph-' ~ paragraph.id(),
  'container',
  paragraph.bundle|clean_class,
  view_mode ? view_mode|clean_class,
]
%}
{% block paragraph %}
  <div{{ attributes.addClass(classes) }}>
    {{ title_suffix.contextual_links }}
    {% block content %}
      {{ content }}
    {% endblock %}
  </div>
{% endblock paragraph %}